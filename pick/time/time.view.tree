$mol_pick_time $mol_pick
	Icon $mol_icon_clock
	-
	bubble_content /
		<= Input $mol_format
			value? <=> value? \
			mask \__:__
			enabled <= enabled true
		<= Pickers $mol_row
			sub /
				<= Hours $mol_switch
					value? <=> hour_selected? \
					options <= hour_options *
				<= Delimiter $mol_paragraph
					title \:
				<= Minutes $mol_switch
					value? <=> minute_selected? \
					options <= minute_options *
	-
	value_moment? $mol_time_moment
